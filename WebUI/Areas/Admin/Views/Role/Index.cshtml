@model IEnumerable<IdentityRole>

<a href="/admin/role/create" class="btn btn-dark">Create</a>
<table class="table table-dark">
    <thead>
        <tr>
            <th>#</th>
            <th>Role</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td></td>
            </tr>
        }
    </tbody>
</table>