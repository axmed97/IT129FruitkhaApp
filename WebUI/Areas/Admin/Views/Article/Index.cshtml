@model IEnumerable<Article>
<a href="/admin/article/create" class="btn btn-dark">Create</a>

<table class="table table-dark ">
    <tr>
        <th>#</th>
        <th>Title</th>
        <th>Content</th>
        <th>Image</th>
        <th>Tags</th>
        <th>Category</th>
        <th>Actions</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
            <td>@Html.Raw(item.Content)</td>
            <td>
                <img src="@item.PhotoUrl" width="200" alt="" />
            </td>
            <td>
                @foreach (var tag in item.ArticleTags)
                {
                    <span>@tag.Tag.Name /</span> 
                }
            </td>
            <td>
                @item.ArticleCategory.Name
            </td>
            <td>
                <a href="/admin/article/edit/@item.Id">edit</a>
            </td>
        </tr>
    }
</table>